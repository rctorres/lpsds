    "name": "lpsds",
    "dockerComposeFile": ["./docker-compose.yml"],
    "service": "main",
    "workspaceFolder": "/work",
    "shutdownAction": "stopCompose",
    "extensions": [
        "ms-python.python",
        "ms-toolsai.jupyter",
        "littlefoxteam.vscode-python-test-adapter",
        "ex-codes.pine-script-syntax-highlighter"
